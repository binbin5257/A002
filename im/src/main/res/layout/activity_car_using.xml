<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/main_view">

    <include layout="@layout/topbar"/>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:background="@color/white">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#1A3C58"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/current_amount"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="16dp"
                        android:layout_marginTop="8dp"
                        android:textColor="@color/white" />

                    <ImageView
                        android:id="@+id/iv_car"
                        android:layout_width="180dp"
                        android:layout_height="90dp"
                        android:layout_below="@+id/current_amount"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="2dp"
                         />

                    <RelativeLayout
                        android:id="@+id/progress_life"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/iv_car"
                        >

                        <cn.lds.im.view.widget.ProgressLayout
                            android:id="@+id/progresslayout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            />


                        <TextView
                            android:id="@+id/tv_life"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/progresslayout"
                            android:layout_centerHorizontal="true"
                            android:textSize="12sp"
                            android:textColor="@color/white"
                            />
                        <LinearLayout
                            android:id="@+id/ll_car_model"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_below="@id/tv_life"
                            android:layout_marginTop="4dp">
                            <RelativeLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1">
                                <TextView
                                    android:id="@+id/car_model"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textSize="@dimen/txt_size_15"
                                    android:textColor="@color/white"
                                    android:drawablePadding="8dp"
                                    android:layout_alignParentRight="true"
                                    android:drawableLeft="@drawable/ic_car"/>
                            </RelativeLayout>

                            <TextView
                                android:id="@+id/car_no"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/txt_size_15"
                                android:textColor="@color/white"
                                android:drawableLeft="@drawable/ic_car_no"
                                android:drawablePadding="8dp"
                                android:layout_weight="1"
                                android:layout_marginLeft="28dp"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/ll_car_usage"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_below="@id/ll_car_model"
                            android:layout_marginTop="4dp">
                            <TextView
                                android:id="@+id/tv_distance"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:textSize="13sp"
                                android:layout_weight="1"
                                android:textColor="@color/white"
                                android:layout_alignParentRight="true"
                                android:gravity="right"/>

                            <TextView
                                android:id="@+id/use_time"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:textColor="@color/white"
                                android:drawablePadding="8dp"
                                android:textSize="13sp"
                                android:layout_weight="1"
                                android:layout_marginLeft="28dp"/>
                        </LinearLayout>

                    </RelativeLayout>


                </RelativeLayout>

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/icon_curve" />

            <RelativeLayout
                android:id="@+id/rl_retrun_car"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:animateLayoutChanges="true"

                >
                <RelativeLayout
                    android:id="@+id/rl_retrun_car_owner"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:visibility="gone">
                    <ImageView
                        android:layout_marginTop="8dp"
                        android:id="@+id/ic_location_owner"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/icon_order_location"
                        android:layout_marginLeft="16dp"
                        />
                    <TextView
                        android:id="@+id/_yard_owner"
                        android:layout_marginTop="10dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="自还车点："
                        android:textSize="12sp"
                        android:layout_toRightOf="@id/ic_location_owner"
                        android:layout_marginLeft="8dp"
                        />
                    <TextView
                        android:id="@+id/tv_yard_owner"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="14sp"
                        android:layout_toRightOf="@id/_yard_owner"
                        android:layout_alignBottom="@id/_yard_owner"
                        android:visibility="gone"
                        />
                    <TextView
                        android:id="@+id/tv_address_owner"
                        android:layout_width="180dp"
                        android:layout_height="wrap_content"
                        android:textSize="12sp"
                        android:layout_marginTop="10dp"
                        android:layout_toRightOf="@id/_yard_owner"
                        android:maxLines="2"
                        android:ellipsize="end"
                        android:layout_marginBottom="10dp"
                        />
                    <View
                        android:layout_width="180dp"
                        android:layout_height="5dp"
                        android:background="@drawable/dash_line"
                        android:layerType="software"
                        android:layout_toRightOf="@id/_yard_owner"
                        android:layout_below="@+id/tv_address_owner"/>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/rl_retrun_car_old"
                    android:layout_below="@+id/rl_retrun_car_owner"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:layout_marginTop="8dp"
                        android:id="@+id/ic_location"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/icon_order_location"
                        android:layout_marginLeft="16dp"
                        />

                    <TextView
                        android:id="@+id/_yard"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="8dp"
                        android:layout_marginTop="10dp"
                        android:layout_toRightOf="@id/ic_location"
                        android:text="还车场站："
                        android:textSize="12sp" />
                    <TextView
                        android:id="@+id/tv_yard"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="14sp"
                        android:layout_toRightOf="@id/_yard"
                        android:layout_marginTop="9dp"
                        />
                    <TextView
                        android:id="@+id/tv_address"
                        android:layout_width="170dp"
                        android:layout_height="wrap_content"
                        android:textSize="12sp"
                        android:layout_marginTop="4dp"
                        android:layout_toRightOf="@id/_yard"
                        android:maxLines="2"
                        android:ellipsize="end"
                        android:layout_below="@+id/tv_yard"
                        android:layout_marginBottom="8dp"
                        />
                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/ll"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="24dp"
                    android:layout_centerVertical="true"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp">
                    <TextView
                        android:id="@+id/tv_navgation"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:drawableTop="@drawable/ic_navgation"
                        android:text="导航"
                        android:textColor="#10BFAF"
                        android:layout_below="@+id/tv_return_car"
                        android:layout_marginTop="10dp"
                        />

                    <TextView
                        android:id="@+id/tv_return_car"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:drawableTop="@drawable/ic_worker"
                        android:text="外勤"
                        android:textColor="#10BFAF"
                        android:visibility="gone"
                        />
                </RelativeLayout>
                <TextView
                    android:visibility="gone"
                    android:id="@+id/prompt_return_location"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toLeftOf="@+id/ll"
                    android:layout_marginRight="12dp"
                    android:layout_alignBottom="@+id/ll"
                    android:drawableTop="@drawable/icon_return_edit"
                    android:textSize="12dp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="#C8C8C8"
                    android:layout_alignParentBottom="true"/>
            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="217dp"
                android:orientation="vertical"
                android:background="@color/white">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    >
                    <RelativeLayout
                        android:id="@+id/rl_open_door"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@drawable/text_bg"
                        android:layout_gravity="center">
                        <ImageView
                            android:id="@+id/iv_open_door"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/ic_unlock"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="16dp"/>
                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_toRightOf="@+id/iv_open_door"
                            android:layout_marginLeft="8dp"
                            >
                            <TextView
                                android:id="@+id/tv_open_chinese"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="车辆开锁"
                                android:textSize="16sp"
                                />
                        </RelativeLayout>


                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:background="#C8C8C8"
                            android:layout_alignParentBottom="true"/>
                        <View
                            android:layout_width="0.5dp"
                            android:layout_height="match_parent"
                            android:background="#C8C8C8"
                            android:layout_alignParentRight="true"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/rl_close_door"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:background="@drawable/text_bg"
                        android:layout_weight="1">
                        <ImageView
                            android:id="@+id/iv_close_door"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/ic_lock"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="16dp"/>
                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_toRightOf="@+id/iv_close_door"
                            android:layout_marginLeft="8dp"
                            >
                            <TextView
                                android:id="@+id/tv_close_chinese"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="车辆关锁"
                                android:textSize="16sp"
                                />
                        </RelativeLayout>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:background="#C8C8C8"
                            android:layout_alignParentBottom="true"/>

                    </RelativeLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1">
                    <RelativeLayout
                        android:id="@+id/rl_whistle"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:background="@drawable/text_bg"
                        android:layout_weight="1">
                        <ImageView
                            android:id="@+id/iv_whistle"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/ic_whisetle"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="16dp"/>
                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_toRightOf="@+id/iv_whistle"
                            android:layout_marginLeft="8dp"
                            >

                            <TextView
                                android:id="@+id/tv_whistle"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="鸣笛闪灯"
                                android:textSize="16sp" />
                        </RelativeLayout>
                        <View
                            android:layout_width="0.5dp"
                            android:layout_height="match_parent"
                            android:background="#C8C8C8"
                            android:layout_alignParentRight="true"/>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:background="#C8C8C8"
                            android:layout_alignParentBottom="true"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/rl_bluetooth"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:background="@drawable/text_bg"
                        android:layout_weight="1">
                        <ImageView
                            android:id="@+id/iv_bluetooth"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/bg_bluetooth_close"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="16dp"/>
                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_toRightOf="@+id/iv_bluetooth"
                            android:layout_marginLeft="8dp"
                            >
                            <TextView
                                android:id="@+id/tv_bluetooth"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="蓝牙开关"
                                android:textSize="16sp"
                                />
                        </RelativeLayout>
                        <View
                            android:layout_width="0.5dp"
                            android:layout_height="match_parent"
                            android:background="#C8C8C8"
                            android:layout_alignParentRight="true"/>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:background="#C8C8C8"
                            android:layout_alignParentBottom="true"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/rl_lamp"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:background="@drawable/text_bg"
                        android:layout_weight="1"
                        android:visibility="gone">
                        <ImageView
                            android:id="@+id/iv_lamp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/ic_lamp"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="16dp"/>
                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_toRightOf="@+id/iv_lamp"
                            android:layout_marginLeft="8dp"
                            >
                            <TextView
                                android:id="@+id/tv_lamp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="闪灯寻车"
                                android:textSize="16sp"
                                />
                        </RelativeLayout>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:background="#C8C8C8"
                            android:layout_alignParentBottom="true"/>
                    </RelativeLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"

                    >

                    <RelativeLayout
                        android:id="@+id/drop_closed"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@drawable/text_bg"
                        android:layout_gravity="center">
                        <ImageView
                            android:id="@+id/iv_drop_closed"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/ic_unlock"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="16dp"/>
                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_toRightOf="@+id/iv_drop_closed"
                            android:layout_marginLeft="8dp"
                            >
                            <TextView
                                android:id="@+id/tv_drop_closed"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="下降车位锁"
                                android:textSize="16sp"
                                />
                        </RelativeLayout>


                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:background="#C8C8C8"
                            android:layout_alignParentBottom="true"/>
                        <View
                            android:layout_width="0.5dp"
                            android:layout_height="match_parent"
                            android:background="#C8C8C8"
                            android:layout_alignParentRight="true"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1">

                    </RelativeLayout>
                    </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#F7F7F7"
                android:orientation="vertical"
                android:animateLayoutChanges="true">
                <RelativeLayout
                    android:visibility="gone"
                    android:id="@+id/rl_pay_way"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginTop="10dp"
                    android:background="@color/white">

                    <TextView
                        android:id="@+id/tv_come_door_car"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="16dp"
                        android:text="上门取车" />
                    <TextView
                        android:id="@+id/tv_come_door_car_suc"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="16dp"
                        android:text="成功预约上门取车！请在约定时间到达指定地点！"
                        android:layout_centerInParent="true"
                        android:visibility="invisible"/>

                    <ImageView
                        android:id="@+id/iv_switch"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:src="@drawable/ic_switch_off"
                        android:layout_centerInParent="true"
                        android:layout_marginRight="16dp"
                        />

                </RelativeLayout>
                <LinearLayout
                    android:id="@+id/ll_return_info"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#F7F7F7"
                    android:visibility="gone"
                    android:animateLayoutChanges="true"
                    android:orientation="vertical">
                    <RelativeLayout
                        android:id="@+id/rl_return_address"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_marginTop="0.5dp"
                        android:background="@drawable/text_bg">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="16dp"
                            android:text="选择还车地点" />
                        <TextView
                            android:id="@+id/tv_return_car_address"
                            android:layout_width="180dp"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:lines="1"
                            android:ellipsize="end"
                            android:gravity="right"
                            android:layout_toLeftOf="@+id/address_arrow_right"/>
                        <ImageView
                            android:id="@+id/address_arrow_right"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:src="@drawable/ic_arrow_right"
                            android:layout_centerVertical="true"
                            />

                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/rl_return_time"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_marginTop="0.5dp"
                        android:background="@drawable/text_bg">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="16dp"
                            android:text="还车时间" />
                        <TextView
                            android:id="@+id/tv_return_car_time"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toLeftOf="@+id/time_arrow_right"
                            android:layout_centerVertical="true"
                            />

                        <ImageView
                            android:id="@+id/time_arrow_right"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:src="@drawable/ic_arrow_right"
                            android:layout_centerVertical="true"
                            />

                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/rl_contract_way"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_marginTop="0.5dp"
                        android:background="@color/white">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="16dp"
                            android:text="联系方式" />

                        <EditText
                            android:id="@+id/et_phone"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@null"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="8dp"
                            android:textSize="14sp"
                            android:inputType="number"
                            android:maxLength="11"
                            android:hint="输入常用手机号"
                            android:gravity="center_vertical|right"
                            />

                </RelativeLayout>
                </LinearLayout>
                <TextView
                    android:id="@+id/pay_get_car"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:gravity="right"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="16dp"
                    android:textColor="#727272"
                    android:textSize="@dimen/txt_size_12"/>
                <TextView
                    android:visibility="invisible"
                    android:id="@+id/tv_waring"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="（注：上门取车请提前预约）"
                    android:textColor="#ff0000"
                    android:gravity="center"
                    android:layout_marginTop="10dp"/>
               <!-- <Button
                    android:id="@+id/btn_return_car"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:background="@drawable/btn_confirm_bg"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"
                    android:layout_marginTop="10dp"
                    android:text="立即还车"
                    android:padding="8dp"
                    android:textColor="@color/white"
                    />-->
                <View
