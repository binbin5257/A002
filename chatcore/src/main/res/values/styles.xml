    <style name="RootTheme" parent="android:Theme.Holo"></style>
    <!-- 通讯录 start -->
    <style name="main_tab">
        <item name="android:layout_width">48dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:textSize">12dp</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:button">@null</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@drawable/tab_textcolor</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:drawablePadding">3dp</item>

    </style>

    <!-- TabHost标签按钮样式 -->
    <style name="tab_button_bottom">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:button">@null</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">16dp</item>
        <item name="android:paddingTop">6dip</item>
        <item name="android:paddingBottom">6dip</item>
        <item name="android:paddingLeft">20dip</item>
        <item name="android:paddingRight">20dip</item>
    </style>

    <style name="person_info_image_button">
        <item name="android:layout_width">40dip</item>
        <item name="android:layout_height">40dip</item>
        <item name="android:scaleType">centerCrop</item>
    </style>

    <style name="person_info_text_buttonTitle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">2dip</item>
        <item name="android:textColor">@color/grey</item>
        <item name="android:textSize">14dip</item>
    </style>

    <style name="person_info_layout_itemLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">43dip</item>
        <item name="android:background">#FFFFFF</item>
        <item name="android:paddingTop">5dip</item>
        <item name="android:paddingBottom">5dip</item>
        <item name="android:paddingLeft">10dip</item>
    </style>

    <style name="common_linear_item_line">
        <item name="android:layout_width">match_parent</item>
